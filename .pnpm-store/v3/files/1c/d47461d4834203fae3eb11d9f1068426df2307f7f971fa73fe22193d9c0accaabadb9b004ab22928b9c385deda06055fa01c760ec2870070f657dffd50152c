import type { type } from 'arktype';
import { type ValidationAdapter, type RequiredDefaultsOptions, type Infer, type InferIn, type ClientValidationAdapter } from './adapters.js';
export declare const arktype: <T extends type.Any>(schema: T, options: RequiredDefaultsOptions<Infer<T>>) => ValidationAdapter<Infer<T>, InferIn<T>>;
export declare const arktypeClient: <T extends type.Any>(schema: T) => ClientValidationAdapter<Infer<T>, InferIn<T>>;
